routine add(x integer, y integer): integer is
    return x + y
end

routine sub(x integer, y integer): integer is
    return x - y
end

routine mul(x integer, y integer): integer is
    return x * y
end

routine div(x integer, y integer): integer is
    return x / y;
end

routine mod(x integer, y integer): integer is
    return x % y;
end