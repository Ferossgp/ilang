type Point is record
    var x: integer
    var y: integer
end

type Pointf is record
    var x: real
    var y: real
end

routine new_point(x integer, y integer): Point is
    var p: Point
    p.x := x
    p.y := y
    return p
end

routine new_pointf(x real, y real): Pointf is
    var pf: Pointf
    pf.x := x
    pf.y := y
    return pf
end
